{
    "name": "veilarbvisittkortfs",
    "version": "0.1.0",
    "type": "module",
    "private": true,
    "scripts": {
        "start": "vite --mode mock",
        "build": "tsc && vite build",
        "build:dev": "tsc && NODE_ENV=development vite build",
        "build:mock": "tsc && vite build --mode mock --base=/veilarbvisittkortfs/",
        "serve": "vite preview",
        "test": "vitest",
        "prettier": "prettier --write 'src/**/*.ts{,x}'",
        "lint": "eslint src",
        "prepare": "husky",
        "tsc": "tsc"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie < 11",
        "not op_mini all"
    ],
    "resolutions": {
        "**/fsevents": "^1.2.9"
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "prettier --write",
            "eslint --config eslint.config.lintstaged.js"
        ]
    },
    "dependencies": {
        "@amplitude/analytics-browser": "2.11.1",
        "@navikt/aksel-icons": "6.16.2",
        "@navikt/ds-css": "6.16.2",
        "@navikt/ds-react": "6.16.2",
        "@navikt/navspa": "6.1.1",
        "axios": "1.7.7",
        "classnames": "2.5.1",
        "constate": "3.3.2",
        "dayjs": "1.11.13",
        "formik": "2.4.6",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "swr": "^2.2.5"
    },
    "devDependencies": {
        "@eslint/js": "9.10.0",
        "@testing-library/react": "16.0.1",
        "@types/react": "18.3.5",
        "@types/react-dom": "18.3.0",
        "@vitejs/plugin-react": "4.3.1",
        "cross-env": "7.0.3",
        "eslint": "8.57.0",
        "eslint-plugin-jsx-a11y": "6.9.0",
        "eslint-plugin-react-hooks": "4.6.2",
        "globals": "15.9.0",
        "husky": "9.1.6",
        "jsdom": "25.0.0",
        "less": "4.2.0",
        "lint-staged": "15.2.10",
        "msw": "2.3.5",
        "prettier": "3.3.3",
        "typescript": "5.6.2",
        "typescript-eslint": "7.11.0",
        "vite": "5.4.2",
        "vite-plugin-svgr": "4.2.0",
        "vitest": "2.1.1"
    },
    "msw": {
        "workerDirectory": "public"
    }
}
